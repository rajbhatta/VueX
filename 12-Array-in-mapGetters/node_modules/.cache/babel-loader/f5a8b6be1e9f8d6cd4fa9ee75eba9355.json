":["import Vue from 'vue';\nimport Vuex from 'vuex';\n\nVue.use(Vuex);\n\nexport const store = new Vuex.Store({\n    state: {\n        products: [\n            {name:'Apple', qtn:30},\n            {name:'Ball', qtn:30},\n             {name:'Cat', qtn:30},\n             {name:'Dog', qtn:30}\n             ]\n    },\n    getters:{\n        saleProducts: state =>{\n            var saleProducts=state.products.map(product =>{\n                return {\n                  name: '**'+product.name+'***',\n                  qtn: product.qtn/2\n                }\n              })\n              return saleProducts;\n        }\n    }\n\n});"]}]}